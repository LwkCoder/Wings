<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.lwkandroid.wingsdemo"
          xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".app.DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="n">
        <activity
            android:name=".project.ycelement.ImageDetailActivity"
            android:screenOrientation="portrait"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity android:name=".project.encrypt.EncryptActivity">
        </activity>
        <activity
            android:name=".project.MainActivity"
            android:theme="@style/BaseActivityStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".project.rxhttp.RxHttpDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.image.ImageLoaderDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.pop.PopDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.qrcode.QRCodeDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.list.ListDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.ycelement.ImageGridActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.dialog.DialogDemoActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/BaseActivityStyle">
        </activity>
    </application>

</manifest>