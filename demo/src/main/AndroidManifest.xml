<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.lwkandroid.wingsdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".app.DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".project.MainActivity"
            android:theme="@style/BaseActivityStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".project.rxhttp.RxHttpDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.image.ImageLoaderDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.pop.PopDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.qrcode.QRCodeDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
        <activity
            android:name=".project.list.ListDemoActivity"
            android:theme="@style/BaseActivityStyle">
        </activity>
    </application>

</manifest>